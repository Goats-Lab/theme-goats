media img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n    transition: transform .6s ease;\n  }\n\n  .mc-col-car__card:hover img {\n    transform: scale(1.05);\n  }\n\n  .mc-col-car__overlay {\n    position: absolute;\n    inset: 0;\n    background: linear-gradient(180deg, rgba(0,0,0,0) 40%, rgba(0,0,0,.75) 100%);\n  }\n\n  .mc-col-car__body {\n    position: absolute;\n    bottom: 1rem;\n    left: 1rem;\n    right: 1rem;\n  }\n\n  .mc-col-car__pill {\n    display: inline-block;\n    font-size: .6rem;\n    letter-spacing: .15em;\n    text-transform: uppercase;\n    padding: .25rem .7rem;\n    background: rgba(255,255,255,.08);\n    margin-bottom: .6rem;\n    opacity: .8;\n  }\n\n  .mc-col-car__name {\n    margin: 0;\n    font-size: 1rem;\n    font-weight: 500;\n  }\n\n  .mc-col-car__desc {\n    font-size: .85rem;\n    color: rgba(255,255,255,.6);\n    margin: 1rem 1rem 1.2rem;\n    line-height: 1.5;\n  }\n\n  @media (max-width: 768px) {\n    .mc-col-car__head {\n      flex-direction: column;\n      align-items: flex-start;\n      gap: 1.5rem;\n    }\n  }\n</style>\n\n{%- assign handles = \"pandoras, tiffany, colecao-swarovski\" | split: \", \" -%}\n\n<div class=\"mc-col-car\" id=\"mc-col-car-home\">\n  <div class=\"mc-col-car__head\">\n    <div>\n      <div class=\"mc-col-car__kicker\">Cat√°logo MyCharms</div>\n      <h2 class=\"mc-col-car__title\">Pronta Entrega</h2>\n      <p class=\"mc-col-car__subtitle\">\n        Explore por marca e categoria e descubra sua pr√≥xima joia.\n      </p>\n    </div>\n\n    <div class=\"mc-col-car__arrows\">\n      <button class=\"mc-col-car__arrow-btn\" data-dir=\"-1\">‚Äπ</button>\n      <button class=\"mc-col-car__arrow-btn\" data-dir=\"1\">‚Ä∫</button>\n    </div>\n  </div>\n\n  <div class=\"mc-col-car__rail\" data-rail>\n    {%- for handle in handles -%}\n      {%- assign coll = collections[handle] -%}\n      {%- if coll and coll.featured_image -%}\n        <a href=\"{{ coll.url }}\" class=\"mc-col-car__card\">\n          <div class=\"mc-col-car__media\">\n            {{ coll.featured_image | image_url: width: 600 | image_tag: loading: 'lazy', draggable: 'false' }}\n            <div class=\"mc-col-car__overlay\"></div>\n\n            <div class=\"mc-col-car__body\">\n              <span class=\"mc-col-car__pill\">{{ coll.title }}</span>\n              <p class=\"mc-col-car__name\">{{ coll.title }}</p>\n            </div>\n          </div>\n\n          {%- if coll.description != blank -%}\n            <p class=\"mc-col-car__desc\">\n              {{ coll.description | strip_html | truncate: 90 }}\n            </p>\n          {%- endif -%}\n        </a>\n      {%- endif -%}\n    {%- endfor -%}\n  </div>\n</div>\n\n<script>\n  (() => {\n    const root = document.querySelector('#mc-col-car-home');\n    if (!root) return;\n\n    const rail = root.querySelector('[data-rail]');\n    const buttons = root.querySelectorAll('.mc-col-car__arrow-btn');\n\n    buttons.forEach(btn => {\n      btn.addEventListener('click', () => {\n        const dir = parseInt(btn.dataset.dir, 10);\n        const card = rail.querySelector('.mc-col-car__card');\n        const amount = card ? card.offsetWidth + 32 : 280;\n        rail.scrollBy({ left: dir * amount, behavior: 'smooth' });\n      });\n    });\n  })();\n</script>",
