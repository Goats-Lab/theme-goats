\n    background: radial-gradient(circle at top left,#1e1237 0,#05030f 40%,#05030f 100%);\n    box-shadow:\n      0 0 0 1px rgba(255,255,255,.02),\n      0 18px 40px rgba(0,0,0,.85),\n      0 0 28px rgba(255,120,210,.22);\n    text-decoration: none;\n    color: #fff;\n    transition: transform .18s ease, box-shadow .18s ease;\n  }\n\n  .mc-col-car__card:hover {\n    transform: translateY(-4px);\n    box-shadow:\n      0 24px 50px rgba(0,0,0,.95),\n      0 0 32px rgba(255,120,210,.5);\n  }\n\n  .mc-col-car__media {\n    position: relative;\n    border-radius: 18px;\n    overflow: hidden;\n    background: #0b0815;\n    aspect-ratio: 4/5;\n    margin-bottom: .7rem;\n  }\n\n  .mc-col-car__media img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n    transform: scale(1.03);\n    transition: transform .4s ease;\n    display: block;\n  }\n\n  .mc-col-car__card:hover .mc-col-car__media img {\n    transform: scale(1.07);\n  }\n\n  .mc-col-car__overlay {\n    position: absolute;\n    inset: 0;\n    background: linear-gradient(\n      180deg,\n      rgba(2,0,12,.05) 0%,\n      rgba(2,0,12,.15) 40%,\n      rgba(2,0,12,.85) 100%\n    );\n  }\n\n  .mc-col-car__body {\n    position: absolute;\n    inset-inline: .85rem;\n    bottom: .7rem;\n    z-index: 1;\n  }\n\n  .mc-col-car__pill {\n    display: inline-flex;\n    align-items: center;\n    padding: 0.08rem .6rem;\n    border-radius: 999px;\n    font-size: .68rem;\n    letter-spacing: .08em;\n    text-transform: uppercase;\n    background: rgba(5,3,18,.9);\n    border: 1px solid rgba(255,163,232,.7);\n    margin-bottom: .25rem;\n  }\n\n  .mc-col-car__name {\n    margin: 0;\n    font-size: .95rem;\n    font-weight: 650;\n  }\n\n  .mc-col-car__desc {\n    margin: .55rem 0 0;\n    font-size: .8rem;\n    color: #f4dcff;\n    opacity: .9;\n  }\n\n  /* --- BOT√ÉO \"VER TODAS AS ENCOMENDAS\" --- */\n  .mc-col-car__cta-wrap {\n    margin-top: 1.4rem;\n    display: flex;\n    justify-content: center;\n  }\n\n  .mc-col-car_